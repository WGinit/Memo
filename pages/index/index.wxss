/**index.wxss**/
.memo{
  width: 100%;
  padding-bottom: 5rem;
  overflow-x: hidden;
 
}
.head{
  font-size: 1rem;
  text-align: center;
  height: 4rem;
  line-height: 4rem;
  border-bottom: 1rpx solid #ddd;
  padding: 0 30rpx;
  
}
.head .left{
  float:left;
}
.head .right{
  float: right;
 
}
.head .iconfont{
  font-size: 1.4rem;
  margin-right:0.2rem
}
.head span, .head text{
  display:inline-block;
  vertical-align:middle;
}
.head .logo{
  display: inline-block;
  background-position-x: left;
  background-position-y: center;
  background-repeat: no-repeat;
  text-indent: 2rem;
  background: url('data:img/png;base64,') no-repeat left;
  background-size: 1.4rem;
}
.head .has-text{
  display: none
}
.head .icon-option{
  color: #e43e2d;
}
.head .icon-edit{
  color: #e43e2d
}
.head .icon-info{
  font-size:1rem;
  margin-left:.2rem;
  color:#666;
}
.head .uncheck{
  display:none;
}
.head .checkAll{
  display:block;
}
.head .icon-del{
  font-size: 1rem;
  color: #e43e2d;
  display: inline-block;
  vertical-align: middle;
}
.head .memo-del{
  font-size: 1rem;
}
.lists{
  width:93%;
  padding: 0 30rpx;
  overflow: hidden;
}
.lists label{
  width:100%;
  display:block;
  padding:40rpx 0;
  border-bottom:1rpx solid #ddd;
  line-height: 1.8rem;
  overflow: hidden;
}
.lists label:last-child{
  border: 0;
}
.lists label text{
  white-space: pre-wrap;
  word-break:break-all; 
  width:100%;
  display:inline-block;

}
.memodetail{
  display: inline-block;
  vertical-align: middle;
  width:82%;
  margin-left:.6rem;
}
.lists label .desc{
  width:100%;
  max-height: 3.5rem;
  word-break:break-all; 
  -webkit-line-clamp: 2;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
}
.lists label .icon-right{
  display: inline-block;
  margin-left:.6rem;
  vertical-align:middle;
  color: #999;
}
.lists label .time{
  display: inline-block;
  font-size: .7rem;
  color: #666;
}
.lists .icon-bq3{
  width:6%;
  float: left;
  color: #f00;
  font-size: 1.1rem;
  margin-top:.1rem;
  overflow: hidden
}
.unedit-checkbox checkbox{
  display:none;
}
.unedit-checkbox label .icon-bq2{
  display: block;
}
.unedit-checkbox .edit-memodesc{
  display:none;
}

.edit-checkbox checkbox {
  width:6%;
  float: left;
  display:block;
  margin-top:.1rem;

}
.edit-checkbox label .icon-bq3{
  display:none;
}
.edit-checkbox .unedit-memodesc{
  display: none;
}
.edit-checkbox .edit-memodesc{
  display: inline-block;
  width:90%;
  margin-left: .6rem;
}
.add-memo{
  display:none;
}
.dellists{
  display: none;

}
.del-btn{
  display:block;
  position: fixed;
  bottom: 0;
  left:0;
  width:100%;
  height: 3rem;
  line-height: 3rem;
  font-size: .8rem;
  background: green;
  color: #fff;
  text-align: center;
  margin:0 auto;
  border-radius:0;
}
.null{
  text-align: center;
  padding-top:5rem;
}
.null-text{
  display: block;
}
.has-text{
  display: none;
}
.null .info{
  font-size: .8rem;
  color: #999;
  text-align: center;
}
.null .info text{
  display: block;
  padding-top: .4rem;
}
.icon-null{
  text-align: center;
  padding-top: 2rem;
  background-position: center;
  font-size: 5rem;
  color: #999
}
